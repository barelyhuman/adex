{
  "name": "adex",
  "version": "0.0.18-alpha.0",
  "description": "An easier way to build apps with Vite and Preact",
  "keywords": [
    "adex",
    "preact",
    "minimal",
    "server",
    "node"
  ],
  "bugs": "github.com/barelyhuman/adex/issues",
  "repository": {
    "type": "git",
    "url": "github.com/barelyhuman/adex"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./ssr": {
      "types": "./src/ssr.d.ts",
      "import": "./src/ssr.js"
    },
    "./types": {
      "types": "./types.d.ts"
    },
    "./head": {
      "types": "./src/head.d.ts",
      "import": "./src/head.js"
    },
    "./hook": {
      "types": "./src/hook.d.ts",
      "import": "./src/hook.js"
    },
    "./http": {
      "types": "./src/http.d.ts",
      "import": "./src/http.js"
    },
    "./env": {
      "types": "./src/env.d.ts",
      "import": "./src/env.js"
    },
    "./fonts": {
      "types": "./src/fonts.d.ts",
      "import": "./src/fonts.js"
    },
    ".": {
      "types": "./src/vite.d.ts",
      "import": "./src/vite.js"
    }
  },
  "files": [
    "src",
    "runtime"
  ],
  "scripts": {
    "next": "bumpp"
  },
  "dependencies": {
    "@barelyhuman/tiny-use": "^0.0.2",
    "@dumbjs/preland": "^0.0.2",
    "bumpp": "^9.4.1",
    "dotenv": "^16.4.5",
    "hoofd": "^1.7.1",
    "mri": "^1.2.0",
    "node-stream-zip": "^1.15.0",
    "ofetch": "^1.4.1",
    "preact-render-to-string": "^6.5.5",
    "regexparam": "^3.0.0",
    "sirv": "^2.0.4",
    "trouter": "^4.0.0",
    "unifont": "^0.0.2",
    "unimport": "^4.0.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.2",
    "@types/node": "^20.14.10",
    "adex-adapter-node": "^0.0.17",
    "autoprefixer": "^10.4.19",
    "tailwindcss": "^3.4.4",
    "vite": "^5.3.1"
  },
  "peerDependenciesMeta": {
    "adex-adapter-node": {
      "optional": true
    }
  },
  "peerDependencies": {
    "@preact/preset-vite": ">=2.8.2",
    "adex-adapter-node": ">=0.0.15",
    "preact": "^10.22.0"
  }
}
