{
  "name": "adex",
  "version": "0.0.19",
  "description": "A minimal, fast framework for building apps with Vite and Preact. Simple SSR, routing, and utilities for modern web projects.",
  "keywords": [
    "adex",
    "preact",
    "vite",
    "ssr",
    "framework",
    "minimal",
    "server",
    "node",
    "web",
    "routing"
  ],
  "homepage": "https://github.com/barelyhuman/adex#readme",
  "bugs": {
    "url": "https://github.com/barelyhuman/adex/issues",
    "email": "ahoy@barelyhuman.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/barelyhuman/adex.git"
  },
  "author": {
    "name": "barelyhuman",
    "email": "ahoy@barelyhuman.dev",
    "url": "https://reaper.is"
  },
  "contributors": [
    {
      "name": "barelyhuman",
      "email": "ahoy@barelyhuman.dev",
      "url": "https://reaper.is"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/barelyhuman"
  },
  "type": "module",
  "main": "./src/vite.js",
  "module": "./src/vite.js",
  "exports": {
    "./package.json": "./package.json",
    "./router": {
      "types": "./src/router.d.ts",
      "import": "./src/router.js"
    },
    "./app": {
      "types": "./src/app.d.ts",
      "import": "./src/app.js"
    },
    "./utils/isomorphic": {
      "types": "./src/utils/isomorphic.d.ts",
      "import": "./src/utils/isomorphic.js"
    },
    "./ssr": {
      "types": "./src/ssr.d.ts",
      "import": "./src/ssr.js"
    },
    "./head": {
      "types": "./src/head.d.ts",
      "import": "./src/head.js"
    },
    "./hook": {
      "types": "./src/hook.d.ts",
      "import": "./src/hook.js"
    },
    "./http": {
      "types": "./src/http.d.ts",
      "import": "./src/http.js"
    },
    "./env": {
      "types": "./src/env.d.ts",
      "import": "./src/env.js"
    },
    "./fonts": {
      "types": "./src/fonts.d.ts",
      "import": "./src/fonts.js"
    },
    ".": {
      "types": "./src/vite.d.ts",
      "import": "./src/vite.js"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "src",
    "runtime"
  ],
  "scripts": {
    "next": "bumpp",
    "test": "glob -c 'node --test --test-reporter=spec' tests/**/*.spec.js",
    "test:ci": "c8 pnpm test"
  },
  "dependencies": {
    "@barelyhuman/tiny-use": "^0.0.2",
    "@dumbjs/preland": "^0.0.2",
    "bumpp": "^9.4.1",
    "dotenv": "^16.4.5",
    "hoofd": "^1.7.1",
    "mri": "^1.2.0",
    "node-stream-zip": "^1.15.0",
    "preact-iso": "^2.9.0",
    "preact-render-to-string": "^6.5.5",
    "regexparam": "^3.0.0",
    "sirv": "^2.0.4",
    "trouter": "^4.0.0",
    "unifont": "^0.0.2"
  },
  "devDependencies": {
    "@barelyhuman/node-snapshot": "^1.0.2",
    "@preact/preset-vite": "^2.8.2",
    "@types/node": "^20.14.10",
    "adex-adapter-node": "^0.0.17",
    "autoprefixer": "^10.4.19",
    "c8": "^10.1.3",
    "glob": "^11.0.1",
    "kolorist": "^1.8.0",
    "tailwindcss": "^3.4.4",
    "vite": "^5.3.1"
  },
  "peerDependenciesMeta": {
    "adex-adapter-node": {
      "optional": true
    }
  },
  "peerDependencies": {
    "@preact/preset-vite": ">=2.8.2",
    "adex-adapter-node": ">=0.0.15",
    "preact": "^10.26.9"
  }
}
